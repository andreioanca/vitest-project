{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "husky install",
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "jest",
    "eject": "react-scripts eject",
    "swagger": "node ./swagger/index.js",
    "lint": "eslint --max-warnings=0 src/**/*.{ts,tsx} tests/**/*.{ts,tsx}",
    "lint:fix": "eslint --fix src/**/*.{ts,tsx} tests/**/*.{ts,tsx}",
    "format": "yarn run lint --fix & yarn prettier --write src/**/*.{ts,tsx} tests/**/*.{ts,tsx}",
    "circ-deps": "npx madge --circular --extensions ts ./src/index.tsx"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
